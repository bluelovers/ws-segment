    "regexp-cjk": "^3.3.35",
    "segment-dict": "^2.3.83",
    "sort-object-keys2": "^2.0.3",
    "str-util": "^2.3.21",
    "ts-enum-util": "4.0.1",
    "ts-type": "^1.2.17",
    "uni-string": "^1.1.2"
  },
