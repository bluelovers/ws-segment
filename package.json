  "directories": {
    "test": "test"
  },
  "workspaces": [
    "packages/@novel-segment/*",
    "packages/*"
  ],
  "scripts": {
    "review:all": "yarn-tool ws run review --concurrency 1",
    "review:since": "yarn run review:all -- --since",
    "coverage:all": "yarn-tool ws run coverage --concurrency 1",
    "coverage:since": "yarn run coverage:all -- --since",
    "lint:all": "yarn-tool ws run lint --concurrency 1",
    "lint:since": "yarn run lint:all -- --since",
    "pretest": "echo pretest",
    "test": "yarn workspace novel-segment run test-ts",
    "test:all": "yarn-tool ws run test --concurrency 1",
    "test:jest:clearCache": "jest --clearCache",
    "test:since": "yarn run test:all -- --since",
    "test:snapshot:all": "yarn-tool ws run test:snapshot --concurrency 1",
    "test:snapshot:since": "yarn run test:snapshot:all -- --since",
    "test:tsd:all": "yarn-tool ws run test:tsd --concurrency 1",
    "test:tsd:since": "yarn run test:tsd:all -- --since",
    "install:frozenLockfile": "yarn-tool install --frozen-lockfile",
    "install:reset-lockfile": "yarn-tool install --reset-lockfile",
    "install:resetLockfile": "yarn-tool install --reset-lockfile",
    "build:all": "yarn-tool ws run build --concurrency 1",
    "build:since": "yarn run build:all -- --since",
    "ci:install": "yarn add -W lerna yarn-tool cjk-conv ts-node typescript ynpx",
    "ci:build": "yarn run ci:build:script && yarn run ci:build:commit",
    "ci:build:commit": "ts-node ./script/run/ci-build.ts",
    "ci:build:script": "ts-node ./packages/novel-segment/test/demo.cache.ts",
    "lerna:publish": "yarn run prepublishOnly:root && lerna publish && yarn run postpublishOnly",
    "lerna:publish:yes": "yarn run prepublishOnly:check-bin && yarn run preversion:segment && lerna publish --yes --bump patch --ignore-changes @novel-segment/api-server --loglevel silly && yarn run postpublishOnly",
    "ws:fix-all": "yarn-tool fix-all  --overwriteHostedGitInfo",
    "ws:fix-all:resetStaticFiles": "yarn run ws:fix-all -- --resetStaticFiles",
    "preversion": "yarn run test",
    "preversion:segment": "ts-node ./script/run/ws-prepublish.ts",
    "version": "echo version",
    "postversion": "yarn-tool fix-all",
    "prepublish": "echo prepublish",
    "prepublish:fix-links": "ynpx @yarn-tool/fix-ws-links",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepare": "echo prepare",
    "prepare:fix-ws-links": "ynpx --quiet @yarn-tool/fix-ws-links",
    "prepublishOnly": "echo prepublishOnly",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:lockfile": "ynpx --quiet sync-lockfile",
    "prepublishOnly:root": "yarn run prepublishOnly:check-bin && yarn run prepare:fix-ws-links",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "prepack": "echo prepack",
    "pack": "echo pack",
    "postpack": "echo postpack",
    "publish": "echo publish",
    "postpublish": "echo postpublish",
    "postpublish:git:subtree": "git subtree push novel-segment-cli master --prefix packages/novel-segment-cli && git subtree push node-segment master --prefix packages/novel-segment && git subtree push node-segment-dict master --prefix packages/segment-dict",
    "postpublish:now-deploy": "ts-node ./script/run/postpublish-now-deploy.ts",
    "postpublish:segment": "ts-node ./script/run/postpublish-segment.ts",
    "postpublish:subtree": "ts-node ./script/run/postpublish-subtree.ts",
    "postpublishOnly": "yarn run postpublish:segment & git push && yarn run postpublish:now-deploy && yarn run postpublish:subtree",
    "postpublishOnly:ws-root-changelog": "ynpx ws-root-changelog & git add ./CHANGELOG.md & git commit ./CHANGELOG.md -m \"chore(changelog): update changelog toc in workspaces root\" & echo update changelog toc in workspaces root",
    "postpublishOnly:add": "ts-node ./script/run/postpublish-add.ts",
    "ncu": "yarn run ncu:ws",
    "ncu:ws": "yarn-tool ncu -u --AA",
    "ncu:root": "yarn-tool ncu -u",
    "sort-package-json": "yarn run sort-package-json:root && yarn run sort-package-json:ws",
    "sort-package-json:root": "yarn-tool sort",
    "sort-package-json:ws": "yarn-tool ws sort",
    "tsc:showConfig": "ynpx get-current-tsconfig -p"
  },
  "resolutions": {},
  "dependencies": {
    "@git-lazy/subtree": "^1.0.32",
    "@lazy-cjk/zh-table-list": "^1.0.83",
    "@lazy-num/sum": "^1.0.8",
    "@lazy-random/fake-lib-r-math-rng": "^1.0.8",
    "@lazy-random/preset-seedrandom": "^1.0.9",
    "@lazy-random/simple-wrap": "^1.0.8",
    "@lazy-random/util-distributions": "^1.0.8",
    "@lazy-random/util-probabilities": "^1.0.8",
    "@lazy-random/util-test": "^1.0.3",
    "@yarn-tool/find-deps": "^2.0.13",
    "@yarn-tool/ws-changed": "^2.0.16",
    "array-hyper-unique": "^2.1.4",
    "bluebird": "^3.7.2",
    "chai": "^4.3.7",
    "chai-asserttype-extra": "^2.0.13",
    "chai-string": "^1.5.0",
    "cjk-conv": "^1.2.142",
    "cross-spawn-extra": "^2.1.0",
    "debug-color2": "^1.3.2",
    "diff": "^5.1.0",
    "fs-extra": "^10.1.0",
    "git-root2": "^1.0.38",
    "jest": "^29.3.1",
    "lib-r-math.js": "^1.0.89",
    "mocha": "^10.1.0",
    "n-readlines": "^1.0.3",
    "prettyuse": "^0.0.0",
    "random-extra": "^4.0.6",
    "regexp-cjk": "^3.3.109",
    "str-util": "^2.3.37",
    "string-natural-compare": "^3.0.1",
    "transliteration": "^2.3.5",
    "uni-string": "^2.0.3",
    "upath2": "^3.1.19",
    "ws-pkg-list": "^2.0.13"
  },
  "devDependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@bluelovers/tsconfig": "^1.0.32",
    "@git-lazy/diff-staged": "^1.0.41",
    "@git-lazy/util": "^1.0.40",
    "@types/benchmark": "^2.1.2",
    "@types/bluebird": "^3.5.37",
    "@types/chai": "^4.3.4",
    "@types/cross-spawn": "^6.0.2",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.189",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/transliteration": "^1.6.6",
    "@types/yargs": "^17.0.13",
    "@yarn-tool/ws-find-up-paths": "*"
  }
}
